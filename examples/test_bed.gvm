//push two values onto the stack;
//then mov each value into a register;
//add value in register r0 to r1;
//and set result back in r0;
//then clear stack before end of program;
func main;
push_i32 3;
push_i32 6;
mov_i32 r0 @$-2;
mov_i32 r1 @$-1;
or r0 r1;
push_i32 r0;
put_i32 @$-1;
pop_i32;
pop_i32;
pop_i32;
ret;